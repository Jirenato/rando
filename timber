local VirtualUser=game:service'VirtualUser'
local userInputService = game:service('UserInputService')
local VIM = game:GetService("VirtualInputManager")
local ScriptOn = false
game:service'Players'.LocalPlayer.Idled:connect(function()
VirtualUser:CaptureController()
VirtualUser:ClickButton2(Vector2.new())
print("Anti-Idle")
end)
userInputService.InputBegan:Connect(function(input, chat)
    if chat then return end
    if input.KeyCode == Enum.KeyCode.KeypadZero then
        if ScriptOn == false then
            ScriptOn = true
            print("ScriptOn")
        else
            ScriptOn = false
            print("ScriptOff")
        end
    end
end)
function FoundTarget()
    local plot = game:GetService("Players").LocalPlayer.Plot.Value
    local y = 1
    local Target = {}
    local HRP = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
    for _,v in pairs(game:GetService("Workspace").Plots:FindFirstChild(plot):GetChildren()) do
        if v:IsA("Model") and v:FindFirstChild("Tree_"..y) and ScriptOn == true then
            table.insert(Target,{v:FindFirstChild("Tree_"..y).Base,(HRP.Position-v:FindFirstChild("Tree_"..y).Base.Position).magnitude})
            y = y + 1
            if y == 4 then y = 1 end
        end
    end
    table.sort(Target,
        function(a,b)
        return a[2]<b[2]
    end)
    return Target
end
function farmtree()
    local TargetInfo = FoundTarget()
    local A_1 = "TreeGrowth"
    local Event = game:GetService("ReplicatedStorage").Communication.Remotes.Upgrade
    Event:FireServer(A_1)
    local A_1 = "AxeStrength"
    Event:FireServer(A_1)
    local A_1 = "WCount"
    Event:FireServer(A_1)
    local A_1 = "WStrength"
    Event:FireServer(A_1)
    local A_1 = "WSpeed"
    Event:FireServer(A_1)
    local A_1 = "WLogs"
    Event:FireServer(A_1)
    repeat
        _G.Speed = 35
        local EnemyLocation = TargetInfo[1][1]
        local distance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - EnemyLocation.Position).magnitude
        tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new((distance/_G.Speed), Enum.EasingStyle.Linear)
        tween = tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(EnemyLocation.Position + EnemyLocation.CFrame.lookVector + Vector3.new(3,0,0), (EnemyLocation.Position))})
        tween:Play()
        game.Players.LocalPlayer.Character:findFirstChildOfClass("Humanoid"):ChangeState(11)
        wait()
    until game:GetService("Players").LocalPlayer:DistanceFromCharacter(TargetInfo[1][1].Position) < 5 or ScriptOn == false
    repeat
        VIM:SendMouseButtonEvent(workspace.CurrentCamera.ViewportSize.X / 2, workspace.CurrentCamera.ViewportSize.Y / 2,0,true,game,0)
        wait()
    until not Target[1][1]
end
while wait() do
    if ScriptOn == true then
        pcall(farmtree)
    end
end
